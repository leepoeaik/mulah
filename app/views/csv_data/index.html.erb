<h1>Table 1</h1>
<table>
  <thead>
    <tr>
      <% @csv_data.headers.each do |header| %>
        <th><%= header %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @csv_data.each do |row| %>
      <tr>
        <% row.each do |value| %>
          <td><%= value[1] %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<h1>Table 2</h1>
<table>
  <thead>
    <tr>
        <th>Category</th>
        <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Alpha</td>
      <td><%= @alpha %></td>
    </tr>
    <tr>
      <td>Beta</td>
      <td><%= @beta %></td>
    </tr>
    <tr>
      <td>Charlie</td>
      <td><%= @charlie %></td>
    </tr>
  </tbody>
</table>

